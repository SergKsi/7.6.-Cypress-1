      "chrome_1600_1200": "npx cypress open --browser chrome --config viewportWidth=1620,viewportHeight=1200",
      "chrome_iphone-se2": "npx cypress open --browser chrome --config viewportWidth=375,viewportHeight=667",
      "config_mobile": "cypress run --config-file cypress.config.js",
      "config_nout": "cypress run --config-file tests/cypress.config.js",
      "chrome_config_mobile_headed": "cypress run --browser chrome --config-file cypress.config.js --headed",
