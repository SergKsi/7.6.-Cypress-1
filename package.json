   "scripts": {
      "edge_1280_720": "npx cypress open --browser edge --config viewportWidth=1280,viewportHeight=720",
      "edge_iphone-x": "npx cypress open --browser edge --config viewportWidth=375,viewportHeight=812",
      "electron_2560_1600": "npx cypress open --browser electron --config viewportWidth=2560,viewportHeight=1600",
