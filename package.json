      "edge_config_mobile_headless": "cypress run --browser edge --config-file cypress.config.js --headless",
      "edge_config_nout_headless": "cypress run --browser edge --config-file tests/cypress.config.js --headless",
      "chrome_config_mobile_headless": "cypress run --browser chrome --config-file cypress.config.js --headless",
      "chrome_config_nout_headless": "cypress run --browser chrome --config-file tests/cypress.config.js --headless"
